        <attr name = "minUnitWid" format="dimension"/>
        <attr name = "maxUnitWid" format="dimension"/>
        <attr name = "unitNums" format="integer"/>
        <attr name = "minUnitColor" format="color"/>
        <attr name = "maxUnitColor" format="color"/>
        <attr name = "startNum" format="float"/>
